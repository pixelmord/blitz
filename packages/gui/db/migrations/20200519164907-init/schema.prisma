generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "***"
}

model Project {
  id           String   @default(cuid()) @id
  createdAt    DateTime @default(now())
  name         String
  path         String
  description  String
  profilePhoto String
  lastActive   DateTime @default(now())
}